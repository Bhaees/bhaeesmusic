{
  "expo": {
    "name": "BhaeBeats",
    "slug": "bhaebeats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./appicon.png",
    "splash": {
      "image": "./appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.bhaees.bhaebeats",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./appicon.png",
        "backgroundColor": "#000000"
      }
    }
  },
  "dependencies": {
    "react-native-svg": "^15.13.0"
  }
}
