{
  "expo": {
    "name": "BhaeBeats",
    "slug": "bhaebeats",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./appicon.png",
    "scheme": "bhaebeats",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {}
    },
    "android": {
      "package": "com.bhaees.bhaebeats"
    },
    "splash": {
      "image": "./appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    }
  }
}
